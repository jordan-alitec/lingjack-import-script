<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Sale Work Orders -->
        <record id="seq_sale_work_order" model="ir.sequence">
            <field name="name">Sale Work Order</field>
            <field name="code">sale.work.order</field>
            <field name="prefix">SWO/%(y)s%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo> 