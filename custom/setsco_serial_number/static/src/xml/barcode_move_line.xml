<?xml version="1.0" encoding="utf-8"?>
<templates xml:space="preserve">
    <t t-name="LineUpperButtons_inherited" t-inherit="stock_barcode.LineUpperButtons" t-inherit-mode="extension">
        <xpath expr="//button[@t-if='imageSource']" position="before">

            <button t-if='line.requires_setsco_serial' class="o_line_button p-0 btn" t-attf-class="btn-{{isSelected || isComplete ? 'light' : 'secondary'}}" t-on-click="onClickAssignSetsconumber">Assign Tuv / Setsco
            </button>
        </xpath>
    </t>


    <t t-name="LineLot_inherit" t-inherit="stock_barcode.LineLot" t-inherit-mode="extension">
        <xpath expr="//div" position="after">
            <div t-if="line.requires_setsco_serial" class="d-flex justify-content-between align-items-center">
                <div name="setsco">
                    <i class="fa fa-fw fa-barcode" t-attf-class="{{nextExpected === 'lot' ? 'o_next_expected' : ''}}"/>
                    <span class="ms-1" t-esc="line.setsco_name"/>
                </div>
            </div>
        </xpath>
    </t>
</templates>
