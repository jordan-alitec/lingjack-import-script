<odoo>

    <record id="show_picking_invoice_form" model="ir.ui.view">
        <field name="name">invoice.form.inherit.add.field.picking</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">

            <notebook position="inside">
                <page string="Delivery Order"
                      invisible="move_type != 'out_invoice' or ac_picking_count == 0">
                    <field name="ac_picking_count" invisible="1"/>
                    <field name="ac_picking_ids" readonly="1"/>
                </page>
            </notebook>
        </field>
    </record>

</odoo>