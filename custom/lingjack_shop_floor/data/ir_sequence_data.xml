<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Manufacturing Order Sequence with PWO25-04-00001 format -->
        <record id="seq_mrp_production_pwo" model="ir.sequence">
            <field name="name">Manufacturing Order PWO</field>
            <field name="code">mrp.production</field>
            <field name="prefix">PWO%(y)s-%(month)s-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="True"/>
            <field name="date_range_ids" eval="[]"/>
        </record>


    </data>
</odoo> 