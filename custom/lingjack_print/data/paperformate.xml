<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="chamber_slip_paperformat" model="report.paperformat">
        <field name="name">Chamber Paper Format</field>
        <field name="default" eval="True"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">128</field>
        <field name="margin_bottom">75</field>
        <field name="margin_left">7</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">124</field>
        <field name="dpi">105</field>
    </record>

    <record id="paperformat_journal_voucher_a5_landscape" model="report.paperformat">
        <field name="name">A5 Landscape</field>
        <field name="default" eval="False"/>
        <field name="format">A5</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">30</field>
        <field name="margin_bottom">18</field>
        <field name="margin_left">7</field>
        <field name="margin_right">7</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">26</field>
        <field name="dpi">103</field>
    </record>

</odoo>